<%= simple_form_for model, html: {multipart: true}, url: form_path do |f| %>
  <%= f.file_field :image %>
  <%= f.input :first_name, label: "First Name" %>
  <%= f.input :last_name, label: "Last Name" %>
  <%= f.input :address, label: "Loacated At" %>
  <%= f.input :placeId, label: "Google Maps PlaceID" %>
  <%= f.input :email %>
  <%= f.input :phone_number %>
  <%= f.input :bio, label: "Bio" %>
  <%= f.button :submit, :class => 'btn-custom2' %>
<% end %>
